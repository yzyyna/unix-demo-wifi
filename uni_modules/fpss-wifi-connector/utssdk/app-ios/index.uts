

/* 引入 interface.uts 文件中定义的变量 */
import { ApiOptionsWifi, wifiFunction } from '../interface.uts';

let wifiConnector = new WiFiConnector()
export const connectWifi : wifiFunction = function (option : ApiOptionsWifi) {
	try {
		wifiConnector.connect(ssid = option.ssid, passphrase = option.pwd, isWEP = false, joinOnce = false, completion = (res : String) => {
			console.log('in uts 结果===', res);
			option.complete?.(res)
		})
	} catch (error) {
		console.log('error ', error);
		//TODO handle the exception
	}
}